{
   "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.tabSize": 4
   },
   "editor.tabSize": 3,
   "editor.detectIndentation": false,
   "editor.wordWrap": "on",
   "editor.formatOnSave": false,
   "editor.cursorBlinking": "phase",
   "editor.renderLineHighlight": "gutter",
   "editor.bracketPairColorization.enabled": false,
   "editor.guides.indentation": true,
   "editor.semanticHighlighting.enabled": true,
   "editor.scrollBeyondLastLine": false,
   "editor.stickyScroll.enabled": true,
   "editor.stickyScroll.maxLineCount": 10,
   "editor.smoothScrolling": true,
   "editor.fontFamily": "JetBrains Mono",
   "editor.fontLigatures": true,
   "editor.fontSize": 16,
   "editor.fontWeight": "500",
   "editor.lineHeight": 22,
   "editor.lineNumbers": "off",
   "editor.minimap.renderCharacters": false,
   "editor.minimap.scale": 2,
   "json.schemaDownload.enable": true,
   "javascript.suggest.autoImports": true,
   "typescript.suggest.autoImports": true,
   "workbench.panel.defaultLocation": "right",
   "workbench.editor.empty.hint": "hidden",
   "workbench.iconTheme": "material-icon-theme",
   "workbench.colorTheme": "One Dark Pro Darker",
   "material-icon-theme.folders.color": "#ddbb77",
   "material-icon-theme.hidesExplorerArrows": true,
   "material-icon-theme.saturation": 1,
   "workbench.colorCustomizations": {
      "minimap.background": "#1e2227",
      "minimapSlider.background": "#00000040",
      "minimapSlider.hoverBackground": "#00000040",
      "minimapSlider.activeBackground": "#00000040",
      "minimap.errorHighlight": "#ff000040",
      "minimap.warningHighlight": "#ff000040",
      "minimap.findMatchHighlight": "#cccccc40",
      "minimap.selectionHighlight": "#474f5e40",
      "minimap.selectionOccurrenceHighlight": "#474f5e40"
   }
}
