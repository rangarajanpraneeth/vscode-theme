{
   "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.tabSize": 4
   },
   "editor.cursorBlinking": "phase",
   "editor.fontFamily": "JetBrains Mono",
   "editor.fontLigatures": true,
   "editor.fontSize": 16,
   "editor.fontWeight": "500",
   "editor.lineHeight": 22,
   "editor.lineNumbers": "off",
   "editor.minimap.renderCharacters": false,
   "editor.minimap.scale": 2,
   "editor.renderLineHighlight": "gutter",
   "editor.tabSize": 3,
   "editor.wordWrap": "on",
   "editor.formatOnSave": false,
   "editor.bracketPairColorization.enabled": false,
   // "editor.guides.bracketPairs": false,
   // "editor.guides.bracketPairsHorizontal": false,
   // "editor.guides.highlightActiveBracketPair": false,
   // "editor.guides.highlightActiveIndentation": false,
   "editor.guides.indentation": true,
   // "editor.links": false,
   // "editor.mouseWheelScrollSensitivity": 1,
   "editor.semanticHighlighting.enabled": true,
   "editor.scrollBeyondLastLine": false,
   "editor.stickyScroll.enabled": true,
   "editor.stickyScroll.maxLineCount": 10,
   // "editor.unicodeHighlight.ambiguousCharacters": false,
   // "editor.unicodeHighlight.invisibleCharacters": false,
   "editor.smoothScrolling": true,
   // "javascript.suggestionActions.enabled": false,
   "json.schemaDownload.enable": true,
   "javascript.suggest.autoImports": true,
   "typescript.suggest.autoImports": true,
   // "security.workspace.trust.untrustedFiles": "open",
   // "terminal.integrated.shellIntegration.decorationsEnabled": "never",
   // "window.customTitleBarVisibility": "windowed",
   // "window.menuBarVisibility": "visible",
   // "window.titleBarStyle": "custom",
   "material-icon-theme.folders.color": "#ddb77b",
   "material-icon-theme.hidesExplorerArrows": true,
   "material-icon-theme.saturation": 1,
   "workbench.panel.defaultLocation": "right",
   "workbench.editor.empty.hint": "hidden",
   "workbench.iconTheme": "material-icon-theme",
   "editor.detectIndentation": false,
   "workbench.colorCustomizations": {
      "minimap.background": "#1e2227",
      "minimap.errorHighlight": "#00000000",
      "minimap.findMatchHighlight": "#00000000",
      "minimap.selectionHighlight": "#00000000",
      "minimap.selectionOccurrenceHighlight": "#00000000",
      "minimap.warningHighlight": "#00000000",
      "minimapSlider.activeBackground": "#00000042",
      "minimapSlider.background": "#00000042",
      "minimapSlider.hoverBackground": "#00000042"
   },
   "workbench.colorTheme": "One Dark Pro Darker",
}
